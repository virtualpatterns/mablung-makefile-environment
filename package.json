{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/**/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "@virtualpatterns/mablung-source-map-support/install"
    ],
    "verbose": true
  },
  "bin": {
    "mablung-makefile-environment": "./release/command/mablung-makefile-environment.js"
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/commonjs/sandbox",
      "release/esmodule/sandbox"
    ],
    "reporter": [
      "html"
    ],
    "src": "release"
  },
  "description": "TBD",
  "eslintConfig": {
    "extends": "@virtualpatterns/eslint-config-mablung-makefile",
    "globals": {
      "__filePath": "readonly",
      "__require": "readonly"
    }
  },
  "exports": {
    "./babel.json": "./babel.config.json",
    "./package.json": "./package.json"
  },
  "files": [
    "/include",
    "/release/command",
    "babel.config.json",
    "makefile"
  ],
  "license": "GPL-3.0+",
  "name": "@virtualpatterns/mablung-makefile-environment",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/mablung-makefile-environment"
  },
  "type": "module",
  "version": "0.0.1-27",
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.14.8",
    "@virtualpatterns/babel-preset-mablung-makefile": "0.0.1-1",
    "@virtualpatterns/eslint-config-mablung-makefile": "0.0.1-12",
    "@virtualpatterns/mablung-babel-plugin-replace-identifier": "0.0.1-31",
    "@virtualpatterns/mablung-babel-plugin-replace-string-literal": "0.0.1-27",
    "@virtualpatterns/mablung-makefile": "0.0.1-93",
    "@virtualpatterns/mablung-source-map-support": "0.0.2-67",
    "commander": "^8.1.0",
    "fs-extra": "^10.0.0",
    "json5": "^2.2.0"
  },
  "devDependencies": {
    "@virtualpatterns/mablung-check-dependency": "0.0.1-63",
    "@virtualpatterns/mablung-worker": "0.0.2-40",
    "ava": "^3.15.0"
  }
}
