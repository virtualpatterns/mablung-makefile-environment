{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/**/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "@virtualpatterns/mablung-source-map-support/install"
    ],
    "verbose": true
  },
  "bin": {
    "mablung-makefile-environment-path": "./release/esmodule/command/mablung-makefile-environment-path.js"
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/commonjs/sandbox",
      "release/commonjs/test",
      "release/esmodule/sandbox",
      "release/esmodule/test",
      "source/esmodule/sandbox",
      "source/esmodule/test"
    ],
    "reporter": [
      "html"
    ]
  },
  "description": "TBD",
  "exports": {
    ".": {
      "import": "./release/esmodule/index.js",
      "require": "./release/commonjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/include",
    "/release/commonjs/command",
    "/release/commonjs/library",
    "/release/commonjs/index.cjs",
    "/release/esmodule/command",
    "/release/esmodule/library",
    "/release/esmodule/index.js",
    "makefile"
  ],
