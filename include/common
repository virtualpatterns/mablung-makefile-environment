
include $(mablung-makefile-path)/include/common

ifndef current-folder

# --------------------
#  version
# --------------------

version::
	$(if $\
		$(shell npx shx which mablung-makefile-environment),$\
		@npx mablung-makefile-environment get-version,$\
		@$(MAKE) run argument="release/command/mablung-makefile-environment.js get-version")

endif